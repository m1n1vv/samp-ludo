	for (new i = 0; i < MAX_LUDO_OBJECTS_WALLS; i++)
	{
		ludo_object = CreateDynamicObject(19379, ludo_map_walls[i][0], ludo_map_walls[i][1], 0.0+LUDO_MAP_NEW_Z, 0.0, 0.0, ludo_map_walls[i][2], -1, LUDO_INTERIOR, -1, LUDO_MAP_DISTANCE, LUDO_MAP_DISTANCE);
		SetDynamicObjectMaterial(ludo_object, 0, 0, " ", " ", 0x00);
	}

	#if defined ludo_map_OnGameModeInit
		ludo_map_OnGameModeInit();
	#endif

	return 1;
}

